datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    Int     @default(autoincrement()) @id
  email String
  password String
  accountType AccountType @default(Local)
}

enum AccountType {
  Local
  Google
  Github
}

